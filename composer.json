{
  "name": "efficiently/authority-controller",
  "description": "AuthorityController is an PHP authorization library for Laravel 5 which restricts what resources a given user is allowed to access.",
  "keywords": [
    "cancan",
    "can",
    "authority",
    "acl",
    "role",
    "permission",
    "ability",
    "allow",
    "deny",
    "rbac",
    "authorization",
    "authentication",
    "security",
    "laravel",
    "laravel 5",
    "laravel 5.1",
    "laravel 5.2",
    "laravel 5.3",
    "rails"
  ],
  "license": "MIT",
  "authors": [{
    "name": "TortueTorche",
    "email": "tortuetorche@spam.me"
  }],
  "require": {
    "php": ">=5.6.4",
    "authority-php/authority-laravel": "~2.4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5",
    "mockery/mockery": "0.9.*",
    "orchestra/testbench": "~3.3.0",
    "laravelcollective/html": "5.3.*"
  },
  "autoload": {
    "files": [
      "src/Efficiently/AuthorityController/helpers.php"
    ],
    "psr-0": {
      "Efficiently\\AuthorityController": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/AcExceptionsHandler.php", "tests/AcTestCase.php", "tests/helpers", "tests/fixtures/",
      "tests/fixtures/models", "tests/fixtures/controllers"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-1.0": "1.0.x-dev",
      "dev-1.1": "1.1.x-dev",
      "dev-1.2": "1.2.x-dev",
      "dev-2.0": "2.0.x-dev",
      "dev-2.1": "2.1.x-dev",
      "dev-2.2": "2.2.x-dev",
      "dev-master": "2.3.x-dev"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "repositories": [
      {
        "name": "authority-php/authority",
        "type": "git",
        "url": "https://github.com/otepas/authority.git"
      }
    ]
}
